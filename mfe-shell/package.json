    "@astrojs/vue": "^4.0.8",
    "@sentry/astro": "^7.99.0",
    "@spotlightjs/astro": "^1.0.13",
    "alpinejs": "^3.13.5",
    "astro": "^4.2.7",
    "astro-htmx": "^1.0.5",
    "concurrently": "^8.2.2",
    "htmx.org": "^1.9.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.5.2",
    "@types/alpinejs": "^3.13.6",
